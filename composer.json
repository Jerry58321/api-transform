{
    "name": "ezp/api-transform",
    "description": "is a api transformation layer",
    "keywords": ["laravel", "api", "transform"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ezp\\ApiTransform\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php" : "^8.0",
        "illuminate/pagination": "^9.0",
        "symfony/http-foundation": "^6.0",
        "illuminate/console": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "symfony/var-dumper": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ezp\\ApiTransform\\TransformServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
